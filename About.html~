				var ldr = new ispring.presenter.loader.FlashPresentationLoader();
				ldr.load("WELD.swf", "content", "FlashPresentation", 960, 578);
				var link = document.getElementById("link");
				ldr.playerPositionChangedEvent().addHandler(function(x, y) {
					link.style.top = (y + 578 + 3) + "px";
				});
			})();
